{
    "name": "jason-guru/laravel-make-repository",
    "description": "A simple package to create repositories and services with artisan command for Laravel 5+",
    "type": "library",
    "require": {
        "php": ">=5.4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Castillo",
            "email": "vicdcastillo@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Vdcastillo\\LaravelMakers\\MakersServiceProvider"
            ]
        }
    },

    "autoload": {
        "psr-4": {
            "Vdcastillo\\LaravelMakers\\": "src/"
        },
        "classmap": ["src/"]
    }
}
